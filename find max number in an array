#include<stdio.h>
#include<conio.h>
main()
{
    int a[20],max,i,n;
    printf("how many elements");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("enter element %d",i+1);
        scanf("%d",&a[i]);
        
    }
    max=a[0];
    for(i=1;i<n;i++)
    {
        if(a[i]>max)
        max=a[i];
    }
    printf("maximum number is %d",max);
    getch();
}
